    <script>
        function filterResults() {
            // Get the date and draw values from dropdowns and input
            let selectedDate = document.getElementById('dateDropdown').value;
            let typedDate = document.getElementById('dateInput').value;
            let selectedDraw = document.getElementById('drawDropdown').value;

            // Use the typed date if provided, otherwise use the dropdown value
            let filterDate = typedDate ? typedDate : selectedDate;

            // Get tables and rows
            let winningNumbersTable = document.getElementById('winningNumbersTable').getElementsByTagName('tbody')[0];
            let winnersTable = document.getElementById('winnersTable').getElementsByTagName('tbody')[0];

            filterTableRows(winningNumbersTable, filterDate, selectedDraw);
            filterTableRows(winnersTable, filterDate, selectedDraw);
        }

        function filterTableRows(tableBody, filterDate, filterDraw) {
            let rows = tableBody.getElementsByTagName('tr');

            for (let i = 0; i < rows.length; i++) {
                let dateCell = rows[i].getElementsByTagName('td')[0];
                let drawCell = rows[i].getElementsByTagName('td')[1];
                if (dateCell && drawCell) {
                    let dateValue = dateCell.textContent || dateCell.innerText;
                    let drawValue = drawCell.textContent || drawCell.innerText;

                    // Show row if both date and draw match the filters
                    if ((filterDate === "" || dateValue.indexOf(filterDate) > -1) &&
                        (filterDraw === "" || drawValue.indexOf(filterDraw) > -1)) {
                        rows[i].style.display = "";
                    } else {
                        rows[i].style.display = "none";
                    }
                }       
            }
        }
    </script>

